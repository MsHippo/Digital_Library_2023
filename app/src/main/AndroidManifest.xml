<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.digitallibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigitalLibrary"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectAudioActivity" />
        <activity android:name=".AudioActivity" />
        <activity android:name=".WishingList.AlterWishingActivity" />
        <activity
            android:name=".WishingList.AddWishingActivity"
            android:label="Adding List" />
        <activity android:name=".WishingList.WishingListActivity" />
        <activity android:name=".BookDetailsActivity" />
        <activity android:name=".MalaysiaLibrary" />
        <activity android:name=".FindBooks" />
        <activity android:name=".EditProfile" />
        <activity android:name=".Login">

        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>